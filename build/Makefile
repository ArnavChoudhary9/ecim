# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug_x64
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_x64)
  ecim_config = debug_x64

else ifeq ($(config),debug_x86)
  ecim_config = debug_x86

else ifeq ($(config),release_x64)
  ecim_config = release_x64

else ifeq ($(config),release_x86)
  ecim_config = release_x86

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := ecim

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

ecim:
ifneq (,$(ecim_config))
	@echo "==== Building ecim ($(ecim_config)) ===="
	@${MAKE} --no-print-directory -C . -f ecim.make config=$(ecim_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f ecim.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_x64"
	@echo "  debug_x86"
	@echo "  release_x64"
	@echo "  release_x86"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   ecim"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"